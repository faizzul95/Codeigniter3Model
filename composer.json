{
    "name": "onlyphp/codeigniter3-model",
    "version": "1.0.2",
    "description": "Advanced model class for CodeIgniter 3 with eager loading and pagination capabilities",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mohd Fahmy Izwan Zulkhafri",
            "email": "faizzul14@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "extra": {
        "installer-paths": {
            "application/core/{$name}/": ["onlyphp/codeigniter3-model"]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "keywords": [
        "my_model",
        "codeigniter",
        "codeigniter3",
        "mysql"
    ]
}